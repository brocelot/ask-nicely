{
  "name": "ask-nicely",
  "version": "2.0.0-rc1",
  "description": "Easily create command-line applications by configuring commands, options and parameters that are supermagically parsed.",
  "author": "wvbe",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git@github.com:wvbe/ask-nicely.git"
  },
  "main": "dist/AskNicely.js",
  "scripts": {
    "test-bare": "mocha test",
    "test": "istanbul cover _mocha",
    "build": "mkdir dist; rollup AskNicely.js --format umd --name ask-nicely | babel --presets es2015 --filename AskNicely.js > dist/AskNicely.js"
  },
  "keywords": [
    "argument",
    "argv",
    "command",
    "option",
    "parameter",
    "parsing",
    "shell",
    "terminal"
  ],
  "directories": {
    "example": "./examples",
    "lib": "./src"
  },
  "engines": {
    "node": ">=4.1.0"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-preset-es2015": "^6.24.1",
    "istanbul": "^0.4.5",
    "mocha": "^3.4.2",
    "rollup": "^0.43.0"
  },
  "babel": {
    "presets": [
      "es2015"
    ]
  }
}
